            <TextBox Width="100" Margin="10,0,0,0" x:Name="editIpAddr" Text="239.255.255.250"/>
            <Label Content="端口:" Margin="10,0,0,0"/>
            <TextBox Width="60" Margin="10,0,0,0" x:Name="editPort" Text="5000"/>

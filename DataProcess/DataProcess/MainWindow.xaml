    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" 
    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" 
    xmlns:dataprocess="clr-namespace:DataProcess" 
    xmlns:System="clr-namespace:System;assembly=mscorlib"    
    xmlns:Controls="clr-namespace:DataProcess.Controls" 
    xmlns:gmap="clr-namespace:DataProcess.GMap"
    x:Class="DataProcess.MainWindow" Title="数据接收解析软件--V 1.0.3" MinHeight="768" MinWidth="1024" WindowState="Maximized" 
    Icon="histroy.ico" Closing="ThemedWindow_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="44*" />
            <ColumnDefinition Width="83*"/>
        </Grid.ColumnDefinitions>
        <ToolBar Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Background="Transparent" Margin="0,0,-0.4,0.4">
            <dx:SimpleButton Content="启动" x:Name="btnStart" Margin="10,0,0,0" Click="btnStart_Click" Glyph="{dx:DXImage SvgImages/XAF/Action_Debug_Start.svg}" />
            <dx:SimpleButton Content="停止" x:Name="btnStop" Margin="10,0,0,0" IsEnabled="False" Glyph="{dx:DXImage SvgImages/XAF/Action_Debug_Stop.svg}" Click="btnStop_Click" />
            <dx:SimpleButton Content="开始存储数据" x:Name="btnData" Margin="10,0,0,0" IsEnabled="False" Glyph="{dx:DXImage SvgImages/Icon Builder/Actions_Database.svg}" Click="btnData_Click"/>
            <dx:SimpleButton Content="设置" x:Name="btnSetting" Margin="10,0,0,0" IsEnabled="True" Glyph="{dx:DXImage SvgImages/Icon Builder/Actions_Settings.svg}" Click="btnSetting_Click" VerticalAlignment="Bottom"/>
            <dx:SimpleButton Content="打开外部数据" x:Name="btnOpenData" Margin="10,0,0,0" Glyph="{dx:DXImage SvgImages/Actions/Open.svg}" Click="btnOpenData_Click"/>
            <dx:SimpleButton Content="数据转换" x:Name="btnDataConvert" Margin="10,0,0,0" Glyph="{dx:DXImage SvgImages/Business Objects/BO_Contract.svg}" Click="btnHistory_Click"/>
            <dx:SimpleButton Content="打开视频软件" x:Name="btnVideo" Margin="10,0,0,0" Click="btnVideo_Click" Glyph="{dx:DXImage Images/Miscellaneous/Video_32x32.png}"/>
            <TextBlock Text="幅值自适应:" Margin="20,0,0,0" VerticalAlignment="Center"/>
            <dxe:ToggleSwitch Margin="0,0,0,0" IsChecked="True" Click="ToggleSwitch_Click" EnableAnimation="False"/>
            <TextBlock Text="缓变参数:" Margin="25,5,5,5"/>
            <Image x:Name="ImageSlow"/>
            <TextBlock Text="速变参数:" Margin="5,5,5,5"/>
            <Image x:Name="ImageFast"/>
            <TextBlock Text="尾段参数:" Margin="5,5,5,5"/>
            <Image x:Name="ImageTail"/>
            <TextBlock Text="飞控数据:" Margin="5,5,5,5"/>
            <Image x:Name="ImageFly"/>
            <TextBlock Text="自主安控:" VerticalAlignment="Center" Margin="5,5,5,5" />
            <Image x:Name="ImageUDP"/>

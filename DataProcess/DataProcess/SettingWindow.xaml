                    <TextBlock Text="尾段内噪声传感器N3系数:" Grid.Row="6" Grid.Column="0" Margin="2,2,2,2" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <dxe:TextEdit x:Name="editNoise" Grid.Row="6" Grid.Column="1" Margin="2,2,2,2" Mask="f13" MaskType="Numeric"/>
                    <TextBlock Text="尾段内噪声传感器N3偏置:" Grid.Row="6" Grid.Column="2" Margin="12,2,2,2" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <dxe:TextEdit x:Name="editNoiseFix" Grid.Row="6" Grid.Column="3" Margin="2,2,2,2" Mask="f13" MaskType="Numeric"/>
                </Grid>
            </dx:DXTabItem>
            <dx:DXTabItem Header="视频软件">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="70"/>
                            <ColumnDefinition Width="300"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="可执行文件:" Grid.Column="0" Margin="2,2,2,2" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <dxe:TextEdit x:Name="editVideoExePath" Grid.Column="1" Margin="20,2,2,2"/>
                        <dx:SimpleButton Grid.Column="2" Content="浏览" Height="20" Margin="15,2,2,2" Glyph="{dx:DXImage SvgImages/Actions/Open.svg}" Click="BtnVideoBrowseClicked"/>
                    </Grid>
                    <Grid Grid.Row="1" Margin="0, 20, 0, 0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="70"/>
                            <ColumnDefinition Width="300"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="运行参数:" Grid.Column="0" Margin="2,2,2,2" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <dxe:TextEdit x:Name="editVideoExeParam" Grid.Column="1" Margin="20,2,2,2"/>
                    </Grid>
                </Grid>
            </dx:DXTabItem>
        </dx:DXTabControl>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <TextBlock Text="最大显示点数:" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center" TextAlignment="Right" Margin="5"/>
            <dxe:SpinEdit  x:Name ="editMaxPoint" Grid.Row="0" Grid.Column="1"  Margin="2,2,2,2" Height="20" Width="80" MinValue="100" MaxValue="10000" Mask="d"/>
        </Grid>
        <Grid Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <dx:SimpleButton Grid.Row="0" Grid.Column="1" Margin="5,5,5,5" Width="100" Content="确定" x:Name="btnOK" Glyph="{dx:DXImage SvgImages/Icon Builder/Actions_CheckCircled.svg}" Click="btnOK_Click"/>
            <dx:SimpleButton Grid.Row="0" Grid.Column="2" Margin="5,5,5,5" Width="100" Content="取消" x:Name="btnCancle" Glyph="{dx:DXImage SvgImages/Icon Builder/Actions_DeleteCircled.svg}" Click="btnCancle_Click"/>
        </Grid>
    </Grid>
</dx:ThemedWindow>

<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" 
             xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
             xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" 
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:cust="clr-namespace:DataProcess.CustomControl"

             xmlns:local="clr-namespace:DataProcess.Controls" 
             x:Class="DataProcess.Controls.YaoCeShuJuXianShi"
             mc:Ignorable="d"
             d:DesignHeight="800" d:DesignWidth="1200">
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="8*"/>
        </Grid.RowDefinitions>
        <ToolBar Background="Transparent" Grid.ColumnSpan="2" Margin="0,0,0,0.4">
            <dx:SimpleButton  x:Name="btnLoad" Margin="0,0,0,4" Glyph="{dx:DXImage SvgImages/Actions/Open.svg}" Click="btnLoad_Click"/>
            <TextBlock Text="系统判决状态" VerticalAlignment="Center" Margin="20,0,0,0" />
            <Image x:Name="imageZhuangTai"/>
            <TextBlock Text="弹头导航" VerticalAlignment="Center" Margin="10,0,0,0" />
            <Image x:Name="imageDanTou" />
            <TextBlock Text="导航快速" VerticalAlignment="Center" Margin="10,0,0,0" />
            <Image x:Name="imageKuaiSu" />
            <TextBlock Text="导航慢速" VerticalAlignment="Center" Margin="10,0,0,0" />
            <Image x:Name="imageManSu" />
            <TextBlock Text="系统状态即时" VerticalAlignment="Center" Margin="10,0,0,0" />
            <Image x:Name="imageJiShi" />

        </ToolBar>
        <dx:DXTabControl Grid.Row="1">
            <dx:DXTabItem Header="系统判决状态">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="6*"/>
                        <ColumnDefinition Width="5*"/>
                    </Grid.ColumnDefinitions>
                    <ScrollViewer>
                        <UniformGrid Rows="18" Columns="3" x:Name="XiTong">
                            <Grid x:Name="XiTong_GNSSTime_Grid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="GNSS时间：" VerticalAlignment="Center"/>
                                <dxe:TextEdit  VerticalAlignment="Center" Grid.Column="1" x:Name="XiTong_GNSSTime" Margin="5" IsReadOnly="True"  BorderThickness="1" BorderBrush="Black">
                                </dxe:TextEdit>
                            </Grid>
                            <cust:LabelTextBox Label="总飞行时间：" x:Name="XiTong_ZongFeiXingTime"/>
                            <Grid/>
                            <cust:LabelTextBox Label="经度：" x:Name="XiTong_JingDu"/>
                            <cust:LabelTextBox Label="纬度：" x:Name="XiTong_WeiDu"/>
                            <cust:LabelTextBox Label="海拔高度：" x:Name="XiTong_GaoDu"/>
                            <cust:LabelTextBox Label="东向速度：" x:Name="XiTong_DongXiangSuDu"/>
                            <cust:LabelTextBox Label="北向速度：" x:Name="XiTong_BeiXiangSuDu"/>
                            <cust:LabelTextBox Label="天向速度：" x:Name="XiTong_TianXiangSuDu"/>
                            <cust:LabelTextBox Label="Wx角速度：" x:Name="XiTong_WxJiaoSuDuValue"/>
                            <cust:LabelTextBox Label="Wy角速度：" x:Name="XiTong_WyJiaoSuDuValue"/>
                            <cust:LabelTextBox Label="Wz角速度：" x:Name="XiTong_WzJiaoSuDuValue"/>
                            <cust:LabelTextBox Label="轴向比力：" x:Name="XiTong_ZhouXiangBiLi"/>
                            <Grid/>
                            <Grid/>
                            <cust:LabelTextBox Label="当前发射系X：" x:Name="XiTong_XFaSheXi"/>
                            <cust:LabelTextBox Label="当前发射系Y：" x:Name="XiTong_YFaSheXi"/>
                            <cust:LabelTextBox Label="当前发射系Z：" x:Name="XiTong_ZFaSheXi"/>
                            <cust:LabelTextBox Label="预示落点Z：" x:Name="XiTong_YuShiLuoDianZ"/>
                            <cust:LabelTextBox Label="预示落点射程：" x:Name="XiTong_YuShiLuoDianSheCheng"/>
                            <Grid/>
                            <cust:LabelTextBox Label="参试状态：" x:Name="XiTong_CanShiZhuangTai"/>
                            <Grid/>
                            <Grid/>
                            <cust:LabelTextBox Label="策略阶段：" x:Name="XiTong_CeLueJieDuan"/>
                            <cust:LabelTextBox Label="紧急断电：" x:Name="XiTong_JinJiDuanDian"/>
                            <cust:LabelTextBox Label="起飞：" x:Name="XiTong_QiFei"/>
                            <cust:LabelTextBox Label="预令：" x:Name="XiTong_YuLing"/>
                            <cust:LabelTextBox Label="动令：" x:Name="XiTong_DongLing"/>
                            <cust:LabelTextBox Label="一级分离：" x:Name="XiTong_YiJiFeiLi"/>
                            <Grid MouseEnter="XiTong_CeLuePanJue1_MouseEnter" x:Name="XiTong_CeLuePanJue1_Grid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="判决结果：" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                <dxe:TextEdit  VerticalAlignment="Center" Grid.Column="1" x:Name="XiTong_CeLuePanJue1" Margin="5" IsReadOnly="True"  BorderThickness="1" BorderBrush="Black">
                                </dxe:TextEdit>
                                <Popup x:Name="XiTong_CeLuePanJue1_PoPup" StaysOpen="False" PlacementTarget="{Binding ElementName=XiTong_CeLuePanJue1_Grid}" Placement="Bottom">
                                    
                                    <Border BorderBrush="SteelBlue" BorderThickness="2,2,2,2" CornerRadius="3" VerticalAlignment="Top">
                                        <TextBlock x:Name="XiTong_CeLuePanJue1_Text" Background="White"/>
                                    </Border >
                                </Popup>
                            </Grid>
                            <Grid MouseEnter="XiTong_CeLuePanJue2_MouseEnter" x:Name="XiTong_CeLuePanJue2_Grid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="实施动作：" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                <dxe:TextEdit  VerticalAlignment="Center" Grid.Column="1" x:Name="XiTong_CeLuePanJue2" Margin="5" IsReadOnly="True"  BorderThickness="1" BorderBrush="Black">
                                </dxe:TextEdit>
                                <Popup x:Name="XiTong_CeLuePanJue2_PoPup" StaysOpen="False" PlacementTarget="{Binding ElementName=XiTong_CeLuePanJue2_Grid}" Placement="Bottom">
                                    
                                    <Border BorderBrush="SteelBlue" BorderThickness="1,1,1,1" CornerRadius="3" VerticalAlignment="Top">
                                        <TextBlock x:Name="XiTong_CeLuePanJue2_Text" Background="White"/>
                                    </Border >
                                </Popup>
                            </Grid>
                            <Grid/>
                            <Grid MouseEnter="XiTong_ShuRuCaiJi1_MouseEnter" x:Name="XiTong_ShuRuCaiJi1_Grid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="解保状态：" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                <dxe:TextEdit  VerticalAlignment="Center" Grid.Column="1" x:Name="XiTong_ShuRuCaiJi1" Margin="5" IsReadOnly="True"  BorderThickness="1" BorderBrush="Black">
                                </dxe:TextEdit>
                                <Popup x:Name="XiTong_ShuRuCaiJi1_PoPup" StaysOpen="False" PlacementTarget="{Binding ElementName=XiTong_ShuRuCaiJi1_Grid}" Placement="Bottom">
                                    <Border BorderBrush="SteelBlue" BorderThickness="1,1,1,1" CornerRadius="3" VerticalAlignment="Top">
                                        <TextBlock x:Name="XiTong_ShuRuCaiJi1_Text" Background="White"/>
                                    </Border >
                                </Popup>
                            </Grid>
                            <Grid MouseEnter="XiTong_ShuRuCaiJi2_MouseEnter" x:Name="XiTong_ShuRuCaiJi2_Grid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="起爆状态：" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                <dxe:TextEdit  VerticalAlignment="Center" Grid.Column="1" x:Name="XiTong_ShuRuCaiJi2" Margin="5" IsReadOnly="True"  BorderThickness="1" BorderBrush="Black">
                                </dxe:TextEdit>
                                <Popup x:Name="XiTong_ShuRuCaiJi2_PoPup" StaysOpen="False" PlacementTarget="{Binding ElementName=XiTong_ShuRuCaiJi2_Grid}" Placement="Bottom">
                                    <Border BorderBrush="SteelBlue" BorderThickness="2,2,2,2" CornerRadius="3" VerticalAlignment="Top">
                                        <TextBlock x:Name="XiTong_ShuRuCaiJi2_Text" Background="White"/>
                                    </Border >
                                </Popup>
                            </Grid>
                            <Grid/>
                            <Grid MouseEnter="XiTong_ShuRuCaiJi3_MouseEnter" x:Name="XiTong_ShuRuCaiJi3_Grid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="配电状态：" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                <dxe:TextEdit  VerticalAlignment="Center" Grid.Column="1" x:Name="XiTong_ShuRuCaiJi3" Margin="5" IsReadOnly="True"  BorderThickness="1" BorderBrush="Black">
                                </dxe:TextEdit>
                                <Popup x:Name="XiTong_ShuRuCaiJi3_PoPup" StaysOpen="False" PlacementTarget="{Binding ElementName=XiTong_ShuRuCaiJi3_Grid}" Placement="Bottom">
                                   
                                    <Border BorderBrush="SteelBlue" BorderThickness="2,2,2,2" CornerRadius="3" VerticalAlignment="Top">
                                        <TextBlock x:Name="XiTong_ShuRuCaiJi3_Text" Background="White"/>
                                    </Border >
                                </Popup>
                            </Grid>
                            <Grid MouseEnter="XiTong_ShuRuCaiJi4_MouseEnter" x:Name="XiTong_ShuRuCaiJi4_Grid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="弹体解保状态：" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                <dxe:TextEdit  VerticalAlignment="Center" Grid.Column="1" x:Name="XiTong_ShuRuCaiJi4" Margin="5" IsReadOnly="True"  BorderThickness="1" BorderBrush="Black">
                                </dxe:TextEdit>
                                <Popup x:Name="XiTong_ShuRuCaiJi4_PoPup" StaysOpen="False" PlacementTarget="{Binding ElementName=XiTong_ShuRuCaiJi4_Grid}" Placement="Bottom">
                                    
                                    <Border BorderBrush="SteelBlue" BorderThickness="2,2,2,2" CornerRadius="3" VerticalAlignment="Top">
                                        <TextBlock x:Name="XiTong_ShuRuCaiJi4_Text" Background="White"/>
                                    </Border >
                                </Popup>
                            </Grid>
                            <Grid/>
                            <cust:LabelTextBox Label="弹头解保：" x:Name="XiTong_DanTouJieBao"/>
                            <cust:LabelTextBox Label="起爆状态：" x:Name="XiTong_QiBaoZhuangTai"/>
                            <Grid/>
                            <cust:LabelTextBox Label="内部控制电压：" x:Name="XiTong_NeiBuKongZhiDian"/>
                            <cust:LabelTextBox Label="功率电压：" x:Name="XiTong_GongLvDian"/>
                            <Grid/>
                            <cust:LabelTextBox Label="导航数据选择：" x:Name="XiTong_DaoHangShuJuXuanZe"/>
                            <cust:LabelTextBox Label="陀螺数据融合：" x:Name="XiTong_TuoLuoShuJuRongHe"/>
                            <cust:LabelTextBox Label="数据更新标识：" x:Name="XiTong_ShuJuWeiGengXin"/>
                            <cust:LabelTextBox Label="优选结果：" x:Name="XiTong_YouXuanJieGuo"/>
                            <cust:LabelTextBox Label="弹头组合：" x:Name="XiTong_DanTouZuHe"/>
                            <cust:LabelTextBox Label="弹体组合：" x:Name="XiTong_DanTiZuHe"/>
                            <Grid MouseEnter="XiTong_ShuJu1Hao_MouseEnter" x:Name="XiTong_ShuJu1Hao_Grid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="1号导航指示：" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                <dxe:TextEdit  VerticalAlignment="Center" Grid.Column="1" x:Name="XiTong_ShuJu1Hao" Margin="5" IsReadOnly="True"  BorderThickness="1" BorderBrush="Black">
                                </dxe:TextEdit>
                                <Popup x:Name="XiTong_ShuJu1Hao_PoPup" StaysOpen="False" PlacementTarget="{Binding ElementName=XiTong_ShuJu1Hao_Grid}" Placement="Bottom">
                                    
                                    <Border BorderBrush="SteelBlue" BorderThickness="2,2,2,2" CornerRadius="3" VerticalAlignment="Top">
                                        <TextBlock x:Name="XiTong_ShuJu1Hao_Text" Background="White"/>
                                    </Border >
                                </Popup>
                            </Grid>
                            <Grid MouseEnter="XiTong_ShuJu2Hao_MouseEnter" x:Name="XiTong_ShuJu2Hao_Grid">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="4*"/>
                                    <ColumnDefinition Width="6*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="2号导航指示：" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                <dxe:TextEdit  VerticalAlignment="Center" Grid.Column="1" x:Name="XiTong_ShuJu2Hao" Margin="5" IsReadOnly="True"  BorderThickness="1" BorderBrush="Black">
                                </dxe:TextEdit>
                                <Popup x:Name="XiTong_ShuJu2Hao_PoPup" StaysOpen="False" PlacementTarget="{Binding ElementName=XiTong_ShuJu2Hao_Grid}" Placement="Bottom">
                                    
                                    <Border BorderBrush="SteelBlue" BorderThickness="2,2,2,2" CornerRadius="3" VerticalAlignment="Top">
                                        <TextBlock x:Name="XiTong_ShuJu2Hao_Text" Background="White"/>
                                    </Border >
                                </Popup>
                            </Grid>
                        </UniformGrid>
                    </ScrollViewer>
                    <dx:DXTabControl Grid.Column="1">
                        <dx:DXTabItem Header="坐标">
                            <UniformGrid Rows="3">
                                <dxc:ChartControl Margin="2,2,2,2">
                                    <dxc:XYDiagram2D  PaneOrientation="Vertical" EnableAxisXNavigation="True" EnableAxisYNavigation="False">
                                        <dxc:XYDiagram2D.AxisY>
                                            <dxc:AxisY2D>
                                                <dxc:AxisY2D.Title>
                                                    <dxc:AxisTitle Content="经度" Visible="True" FontSize="15" />
                                                </dxc:AxisY2D.Title>
                                            </dxc:AxisY2D>
                                        </dxc:XYDiagram2D.AxisY>
                                        <dxc:LineSeries2D  LabelsVisibility="False" MarkerVisible="False" x:Name="chart_XiTong_ZuoBiao_JingDu" MarkerSize="10" ArgumentDataMember="X" ValueDataMember="Y" DisplayName="LineSeries1">
                                            <dxc:LineSeries2D.LineStyle>
                                                <dxc:LineStyle LineJoin="Bevel"/>
                                            </dxc:LineSeries2D.LineStyle>
                                        </dxc:LineSeries2D>
                                    </dxc:XYDiagram2D>
                                </dxc:ChartControl>
                                <dxc:ChartControl Margin="2,2,2,2">
                                    <dxc:XYDiagram2D PaneOrientation="Vertical" EnableAxisXNavigation="True" EnableAxisYNavigation="False" Rotated="False" DependentAxesYRange="{x:Null}" EqualBarWidth="False">
                                        <dxc:XYDiagram2D.AxisY>
                                            <dxc:AxisY2D>
                                                <dxc:AxisY2D.Title>
                                                    <dxc:AxisTitle Content="纬度" Visible="True" FontSize="15" />
                                                </dxc:AxisY2D.Title>
                                            </dxc:AxisY2D>
                                        </dxc:XYDiagram2D.AxisY>
                                        <dxc:LineSeries2D  LabelsVisibility="False" MarkerVisible="False" x:Name="chart_XiTong_ZuoBiao_WeiDu" MarkerSize="10" ArgumentDataMember="X" ValueDataMember="Y" DisplayName="LineSeries1">
                                            <dxc:LineSeries2D.LineStyle>
                                                <dxc:LineStyle LineJoin="Bevel"/>
                                            </dxc:LineSeries2D.LineStyle>
                                        </dxc:LineSeries2D>
                                    </dxc:XYDiagram2D>
                                </dxc:ChartControl>
                                <dxc:ChartControl Margin="2,2,2,2">
                                    <dxc:XYDiagram2D PaneOrientation="Vertical" EnableAxisXNavigation="True" EnableAxisYNavigation="False" Rotated="False" DependentAxesYRange="{x:Null}" EqualBarWidth="False">
                                        <dxc:XYDiagram2D.AxisY>
                                            <dxc:AxisY2D>
                                                <dxc:AxisY2D.Title>
                                                    <dxc:AxisTitle Content="海拔高度" Visible="True" FontSize="15" />
                                                </dxc:AxisY2D.Title>
                                            </dxc:AxisY2D>
                                        </dxc:XYDiagram2D.AxisY>
                                        <dxc:LineSeries2D  LabelsVisibility="False" MarkerVisible="False" x:Name="chart_XiTong_ZuoBiao_GaoDu" MarkerSize="10" ArgumentDataMember="X" ValueDataMember="Y" DisplayName="LineSeries1">
                                            <dxc:LineSeries2D.LineStyle>
                                                <dxc:LineStyle LineJoin="Bevel"/>
                                            </dxc:LineSeries2D.LineStyle>
                                        </dxc:LineSeries2D>
                                    </dxc:XYDiagram2D>
                                </dxc:ChartControl>
                            </UniformGrid>
                        </dx:DXTabItem>
                        <dx:DXTabItem Header="速度">
                            <UniformGrid Rows="3">
                                <dxc:ChartControl Margin="2,2,2,2">
                                    <dxc:XYDiagram2D PaneOrientation="Vertical" EnableAxisXNavigation="True" EnableAxisYNavigation="False" Rotated="False" DependentAxesYRange="{x:Null}" EqualBarWidth="False">
                                        <dxc:XYDiagram2D.AxisY>
                                            <dxc:AxisY2D>
                                                <dxc:AxisY2D.Title>
                                                    <dxc:AxisTitle Content="东向速度" Visible="True" FontSize="15" />
                                                </dxc:AxisY2D.Title>
                                            </dxc:AxisY2D>
                                        </dxc:XYDiagram2D.AxisY>
                                        <dxc:LineSeries2D  LabelsVisibility="False" MarkerVisible="False" x:Name="chart_XiTong_SuDu_Dong" MarkerSize="10" ArgumentDataMember="X" ValueDataMember="Y" DisplayName="LineSeries1">
                                            <dxc:LineSeries2D.LineStyle>
                                                <dxc:LineStyle LineJoin="Bevel"/>
                                            </dxc:LineSeries2D.LineStyle>
                                        </dxc:LineSeries2D>
                                    </dxc:XYDiagram2D>
                                </dxc:ChartControl>
                                <dxc:ChartControl Margin="2,2,2,2">
                                    <dxc:XYDiagram2D PaneOrientation="Vertical" EnableAxisXNavigation="True" EnableAxisYNavigation="False" Rotated="False" DependentAxesYRange="{x:Null}" EqualBarWidth="False">
                                        <dxc:XYDiagram2D.AxisY>
                                            <dxc:AxisY2D>
                                                <dxc:AxisY2D.Title>
                                                    <dxc:AxisTitle Content="北向速度" Visible="True" FontSize="15" />
                                                </dxc:AxisY2D.Title>
                                            </dxc:AxisY2D>
                                        </dxc:XYDiagram2D.AxisY>
                                        <dxc:LineSeries2D  LabelsVisibility="False" MarkerVisible="False" x:Name="chart_XiTong_SuDu_Bei" MarkerSize="10" ArgumentDataMember="X" ValueDataMember="Y" DisplayName="LineSeries1">
                                            <dxc:LineSeries2D.LineStyle>
                                                <dxc:LineStyle LineJoin="Bevel"/>
                                            </dxc:LineSeries2D.LineStyle>
                                        </dxc:LineSeries2D>
                                    </dxc:XYDiagram2D>
                                </dxc:ChartControl>
                                <dxc:ChartControl Margin="2,2,2,2">
                                    <dxc:XYDiagram2D PaneOrientation="Vertical" EnableAxisXNavigation="True" EnableAxisYNavigation="False" Rotated="False" DependentAxesYRange="{x:Null}" EqualBarWidth="False">
                                        <dxc:XYDiagram2D.AxisY>
                                            <dxc:AxisY2D>
                                                <dxc:AxisY2D.Title>
                                                    <dxc:AxisTitle Content="天向速度" Visible="True" FontSize="15" />
                                                </dxc:AxisY2D.Title>
                                            </dxc:AxisY2D>
                                        </dxc:XYDiagram2D.AxisY>
                                        <dxc:LineSeries2D  LabelsVisibility="False" MarkerVisible="False" x:Name="chart_XiTong_SuDu_Tian" MarkerSize="10" ArgumentDataMember="X" ValueDataMember="Y" DisplayName="LineSeries1">
                                            <dxc:LineSeries2D.LineStyle>
                                                <dxc:LineStyle LineJoin="Bevel"/>
                                            </dxc:LineSeries2D.LineStyle>
                                        </dxc:LineSeries2D>
                                    </dxc:XYDiagram2D>
                                </dxc:ChartControl>
                            </UniformGrid>
                        </dx:DXTabItem>
                        <dx:DXTabItem Header="角速度">
                            <UniformGrid Rows="3">
                                <dxc:ChartControl Margin="2,2,2,2">
                                    <dxc:XYDiagram2D PaneOrientation="Vertical" EnableAxisXNavigation="True" EnableAxisYNavigation="False" Rotated="False" DependentAxesYRange="True" EqualBarWidth="False">
                                        <dxc:XYDiagram2D.AxisY>
                                            <dxc:AxisY2D>
                                                <dxc:AxisY2D.Title>
                                                    <dxc:AxisTitle Content="Wx角速度" Visible="True" FontSize="15" />
                                                </dxc:AxisY2D.Title>
                                            </dxc:AxisY2D>
                                        </dxc:XYDiagram2D.AxisY>
                                        <dxc:LineSeries2D  LabelsVisibility="False" MarkerVisible="False" x:Name="chart_XiTong_JiaoSuDu_Wx" MarkerSize="10" ArgumentDataMember="X" ValueDataMember="Y" DisplayName="LineSeries1">
                                            <dxc:LineSeries2D.LineStyle>
                                                <dxc:LineStyle LineJoin="Bevel"/>
                                            </dxc:LineSeries2D.LineStyle>
                                        </dxc:LineSeries2D>
                                    </dxc:XYDiagram2D>

                                </dxc:ChartControl>
                                <dxc:ChartControl Margin="2,2,2,2">
                                    <dxc:XYDiagram2D PaneOrientation="Vertical" EnableAxisXNavigation="True" EnableAxisYNavigation="False" Rotated="False" DependentAxesYRange="{x:Null}" EqualBarWidth="False">
                                        <dxc:XYDiagram2D.AxisY>
                                            <dxc:AxisY2D>

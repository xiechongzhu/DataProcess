                                        </dxc:XYDiagram2D.AxisY>
                                        <dxc:LineSeries2D  LabelsVisibility="False" MarkerVisible="False" x:Name="chart_XiTong_JiaoSuDu_Wy" MarkerSize="10" ArgumentDataMember="X" ValueDataMember="Y" DisplayName="LineSeries1">
                                            <dxc:LineSeries2D.LineStyle>
                                                <dxc:LineStyle LineJoin="Bevel"/>

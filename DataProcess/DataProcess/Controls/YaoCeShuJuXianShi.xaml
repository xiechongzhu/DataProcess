                                    <dxc:XYDiagram2D PaneOrientation="Vertical" EnableAxisXNavigation="True" EnableAxisYNavigation="False" Rotated="False" DependentAxesYRange="{x:Null}" EqualBarWidth="False">
                                        <dxc:XYDiagram2D.AxisY>
                                            <dxc:AxisY2D>
                                                <dxc:AxisY2D.Title>
                                                    <dxc:AxisTitle Content="Wz角速度" Visible="True" FontSize="15" />
                                                </dxc:AxisY2D.Title>
                                            </dxc:AxisY2D>
                                        </dxc:XYDiagram2D.AxisY>
                                        <dxc:LineSeries2D  LabelsVisibility="False" MarkerVisible="False" x:Name="chart_XiTong_JiaoSuDu_Wz" MarkerSize="10" ArgumentDataMember="X" ValueDataMember="Y" DisplayName="LineSeries1">
